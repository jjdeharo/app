{
  "categorias": [
    {
      "nombre": "Básico",
      "id": "basico",
      "grid_template_columns": "repeat(auto-fit, minmax(80px, 1fr))",
      "elementos": [
        {
          "type": "button",
          "latex": "\\frac{}{}",
          "display": "\\frac{a}{b}",
          "title": "Fracción"
        },
        {
          "type": "button",
          "latex": "^{}",
          "display": "x^{a}",
          "title": "Superíndice"
        },
        {
          "type": "button",
          "latex": "_{}",
          "display": "x_{a}",
          "title": "Subíndice"
        },
        {
          "type": "button",
          "latex": "\\sqrt{}",
          "display": "\\sqrt{x}",
          "title": "Raíz cuadrada"
        },
        {
          "type": "button",
          "latex": "\\sqrt[]{}",
          "display": "\\sqrt[n]{x}",
          "title": "Raíz n-ésima"
        },
        {
          "type": "button",
          "latex": "\\vec{}",
          "display": "\\vec{v}",
          "title": "Vector"
        }
      ]
    },
    {
      "nombre": "Operadores",
      "id": "operadores",
      "grid_template_columns": "repeat(auto-fit, minmax(60px, 1fr))",
      "elementos": [
        { "type": "button", "latex": "\\pm", "title": "Más/Menos" },
        { "type": "button", "latex": "\\times", "title": "Multiplicación" },
        { "type": "button", "latex": "\\div", "title": "División" },
        { "type": "button", "latex": "\\leq", "title": "Menor o igual" },
        { "type": "button", "latex": "\\geq", "title": "Mayor o igual" },
        { "type": "button", "latex": "\\neq", "title": "Distinto" },
        { "type": "button", "latex": "\\approx", "title": "Aproximado" },
        { "type": "button", "latex": "\\in", "title": "Pertenece" },
        { "type": "button", "latex": "\\notin", "title": "No pertenece" },
        { "type": "button", "latex": "\\subset", "title": "Subconjunto" },
        { "type": "button", "latex": "\\infty", "title": "Infinito" },
        { "type": "button", "latex": "\\forall", "title": "Para todo" },
        { "type": "button", "latex": "\\exists", "title": "Existe" },
        { "type": "button", "latex": "\\binom{n}{k}", "title": "Binomio" }
      ]
    },
    {
      "nombre": "Griego",
      "id": "griego",
      "grid_template_columns": "repeat(auto-fit, minmax(50px, 1fr))",
      "elementos": [
        { "type": "button", "latex": "\\alpha", "title": "Alpha" },
        { "type": "button", "latex": "\\beta", "title": "Beta" },
        { "type": "button", "latex": "\\gamma", "title": "Gamma" },
        { "type": "button", "latex": "\\delta", "title": "Delta" },
        { "type": "button", "latex": "\\epsilon", "title": "Epsilon" },
        { "type": "button", "latex": "\\zeta", "title": "Zeta" },
        { "type": "button", "latex": "\\eta", "title": "Eta" },
        { "type": "button", "latex": "\\theta", "title": "Theta" },
        { "type": "button", "latex": "\\iota", "title": "Iota" },
        { "type": "button", "latex": "\\kappa", "title": "Kappa" },
        { "type": "button", "latex": "\\lambda", "title": "Lambda" },
        { "type": "button", "latex": "\\mu", "title": "Mu" },
        { "type": "button", "latex": "\\nu", "title": "Nu" },
        { "type": "button", "latex": "\\xi", "title": "Xi" },
        { "type": "button", "latex": "\\pi", "title": "Pi" },
        { "type": "button", "latex": "\\rho", "title": "Rho" },
        { "type": "button", "latex": "\\sigma", "title": "Sigma" },
        { "type": "button", "latex": "\\tau", "title": "Tau" }
      ]
    },
    {
      "nombre": "Límites",
      "id": "limites",
      "grid_template_columns": "repeat(auto-fit, minmax(120px, 1fr))",
      "elementos": [
        { "type": "button", "latex": "\\lim_{x \\to a} f(x)", "title": "Límite" },
        { "type": "button", "latex": "\\sum_{i=0}^{n}", "title": "Sumatorio" },
        { "type": "button", "latex": "\\int_{a}^{b} f(x) dx", "title": "Integral definida" },
        { "type": "button", "latex": "\\frac{dy}{dx}", "title": "Derivada" }
      ]
    },
    {
      "nombre": "Matrices",
      "id": "matrices",
      "grid_template_columns": "repeat(auto-fit, minmax(140px, 1fr))",
      "elementos": [
        { "type": "button", "latex": "\\begin{pmatrix} a & b \\\\ c & d \\end{pmatrix}", "title": "Matriz 2x2" },
        { "type": "button", "latex": "\\begin{bmatrix} a & b \\\\ c & d \\end{bmatrix}", "title": "Matriz 2x2 corchetes" },
        { "type": "button", "latex": "\\begin{vmatrix} a & b \\\\ c & d \\end{vmatrix}", "title": "Determinante 2x2" },
        { "type": "button", "latex": "\\begin{pmatrix} a & b & c \\\\ d & e & f \\\\ g & h & i \\end{pmatrix}", "title": "Matriz 3x3" },
        { "type": "custom_matrix" }
      ]
    },
    {
      "nombre": "Sistemas",
      "id": "sistemas",
      "grid_template_columns": "repeat(auto-fit, minmax(250px, 1fr))",
      "elementos": [
        { "type": "button", "latex": "\\begin{cases} a_1x+b_1y=c_1 \\\\ a_2x+b_2y=c_2 \\end{cases}", "title": "Sistema 2 ecuaciones" },
        { "type": "button", "latex": "\\left\\{ \\begin{array}{l} x + y = 1 \\\\ x - y = 0 \\end{array} \\right.", "title": "Sistema con array" },
        { "type": "button", "latex": "\\begin{cases} a_1x+b_1y+c_1z=d_1 \\\\ a_2x+b_2y+c_2z=d_2 \\\\ a_3x+b_3y+c_3z=d_3 \\end{cases}", "title": "Sistema 3 ecuaciones" },
        { "type": "button", "latex": "\\left\\{ \\begin{array}{l} x + y + z = 1 \\\\ 2x - y + z = 0 \\\\ x + 2y - z = 3 \\end{array} \\right.", "title": "Sistema 3 ecuaciones con array" }
      ]
    }
  ]
}
