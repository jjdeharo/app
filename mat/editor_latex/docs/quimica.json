        {
          "type": "button",
          "latex": "\\ce{(g)}",
          "display": "\\ce{(g)}",
          "title": "Estado gaseoso"
        },
        {
          "type": "button",
          "latex": "\\ce{(aq)}",
          "display": "\\ce{(aq)}",
          "title": "Disuelto en agua"
