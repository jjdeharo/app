    {
      "nombre": "Reacciones químicas",
      "id": "reacciones-quimicas",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "\\ce{2H2 + O2 -> 2H2O}",
