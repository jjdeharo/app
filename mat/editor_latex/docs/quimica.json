        {
          "type": "button",
          "latex": "\\ce{C + O2 -> CO2}",
          "display": "\\ce{C + O2 -> CO2}",
          "title": "Combustión del carbono"
        },
        {
          "type": "button",
          "latex": "\\ce{CaCO3 ->[\\Delta] CaO + CO2}",
          "display": "\\ce{CaCO3 ->[\\Delta] CaO + CO2}",
