        {
          "type": "button",
          "latex": "R = 0.0821\\ \\mathrm{L\\cdot atm\\cdot mol^{-1}\\cdot K^{-1}}",
          "display": "R = 0.0821\\ \\mathrm{L\\cdot atm\\cdot mol^{-1}\\cdot K^{-1}}",
          "title": "Constante de los gases"
        },
        {
          "type": "button",
          "latex": "V_m = 22.4\\ \\mathrm{L/mol}",
          "display": "V_m = 22.4\\ \\mathrm{L/mol}",
          "title": "Volumen molar"
        },
        {
          "type": "button",
          "latex": "1\\ \\mathrm{mol} = 6.022 \\times 10^{23}\\ \\mathrm{part\\acute{\\i}culas}",
          "display": "1\\ \\mathrm{mol} = 6.022 \\times 10^{23}",
          "title": "Número de Avogadro"
        }
      ]
    }
  ]
}