      "elementos": [
        {
          "type": "button",
          "latex": "\\ce{H2O}",
          "display": "\\ce{H2O}",
          "title": "Agua"
        },
        {
          "type": "button",
          "latex": "\\ce{CO2}",
          "display": "\\ce{CO2}",
          "title": "Dióxido de carbono"
        },
        {
          "type": "button",
          "latex": "\\ce{NaCl}",
          "display": "\\ce{NaCl}",
          "title": "Cloruro de sodio"
        },
        {
          "type": "button",
          "latex": "\\ce{O2}",
          "display": "\\ce{O2}",
          "title": "Oxígeno molecular"
        },
        {
          "type": "button",
          "latex": "\\ce{CH4}",
          "display": "\\ce{CH4}",
          "title": "Metano"
        },
        {
          "type": "button",
          "latex": "\\ce{C6H12O6}",
          "display": "\\ce{C6H12O6}",
