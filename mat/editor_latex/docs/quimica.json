        {
          "type": "button",
          "latex": "\\ce{<=>}",
          "display": "\\ce{<=>}",
          "title": "Equilibrio químico"
        }
      ]
    },
    {
      "nombre": "Estequiometría",
      "id": "estequiometria",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": true,
      "elementos": [
        {
          "type": "button",
          "latex": "n = \\frac{m}{M}",
          "display": "n = \\frac{m}{M}",
          "title": "Moles a partir de masa"
        },
        {
          "type": "button",
          "latex": "m = n \\cdot M",
          "display": "m = n \\cdot M",
          "title": "Masa a partir de moles"
        },
        {
          "type": "button",
          "latex": "n = \\frac{V}{V_m}",
          "display": "n = \\frac{V}{V_m}",
          "title": "Moles a partir de volumen"
        },
        {
          "type": "button",
          "latex": "C = \\frac{n}{V}",
          "display": "C = \\frac{n}{V}",
          "title": "Concentración molar"
        }
      ]
    },
    {
      "nombre": "Unidades y constantes",
      "id": "unidades-constantes",
      "grid_template_columns": "repeat(auto-fit, minmax(150px, 1fr))",
